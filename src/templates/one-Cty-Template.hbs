{{#each this}}
<h1>{{this.name}}</h1>
{{/each}}
<div class="wrapper-container">
    <ul class="list-value">
        {{#each this}}
        <li>
            <p>capital: {{this.capital}}</p>
        </li>
        <li>
            <p>population: {{this.population}} </p>
        </li>

        <li>
            <p>languages:</p>
            {{#each languages}}
            <ul>
                <li>
                    <p>{{this.name}}</p>
                </li>
            </ul>
            {{/each}}
        </li>
        </li>
        {{/each}}
    </ul>
    {{#each this}}
    <img src={{this.flag}} alt="" width="300px">
    {{/each}}
</div>

